<div class='bridgetroll-section-title {{#unless destructable}}special{{/unless}} level{{level}}'>
  <span class='title-text' title='{{title}}'>{{title}}</span>
  <div class='actions'>
    {{#if destructable}}
      <i class="fa fa-edit edit" title="Edit"></i>
      <i class='fa fa-close destroy' title="Destroy"></i>
    {{/if}}
  </div>
</div>
<ul class='volunteers'>
{{#each volunteers}}
  <li class="attendee {{#unless this.selected_session_checkins_count}}no-show{{/unless}} role{{this.role_id}}" data-id="{{this.id}}">
    <div class="bridgetroll-badge level{{this.class_level}}" title='{{this.volunteer_letter}}: {{this.teaching_preference_description}}'>{{this.volunteer_letter}}</div>
    <span class='name'>{{this.full_name}}</span>
    <i class='fa fa-info-circle info'></i>
  </li>
{{/each}}
</ul>

<ul class='students'>
{{#each students}}
  <li class="attendee {{#unless this.selected_session_checkins_count}}no-show{{/unless}} {{this.operating_system_type}}" data-id="{{this.id}}">
    <div class="bridgetroll-badge level{{this.class_level}}">{{this.class_level}}</div>
    <span class='name'>{{this.full_name}}</span>
    <i class='fa fa-info-circle info'></i>
  </li>
{{/each}}
</ul>
